{
  "name": "s-mcdonald/chronicle",
  "description": "A Lightweight utility for Dealing with Date and Time in PHP",
  "keywords": ["DateTime" ,"Date", "Time", "PSR-20", "Chronicle"],
  "homepage": "https://github.com/s-mcdonald/Chronicle",
  "license": "MIT",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Sam McDonald",
      "email": "s.mcdonald@outlook.com.au",
      "homepage": "https://github.com/s-mcdonald"
    }
  ],
  "require": {
    "php": ">=8.1",
    "psr/clock": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "mockery/mockery": "1.5.1"
  },
  "autoload": {
    "psr-4": {
      "SamMcDonald\\Chronicle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SamMcDonald\\Chronicle\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
